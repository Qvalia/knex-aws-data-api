{
  "name": "@qvalia/knex-aws-data-api",
  "version": "1.1.0",
  "description": "Knex plugin that uses AWS Data API internally to execute SQL queries. Postgres & Mysql",
  "main": "index.js",
  "files": [
    "src/**/*.js"
  ],
  "scripts": {
    "lint": "eslint src",
    "test-integration": "jest --config=jest.config.js --testMatch **/test/**/*.test.js",
    "test": "jest --config=jest.config.js --testMatch **/src/**/*.test.js",
    "package": "rm -rf node_modules && npm install --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qvalia/knex-aws-data-api.git"
  },
  "keywords": [
    "qvalia",
    "knex",
    "data",
    "api",
    "rds",
    "aurora",
    "serverless",
    "mysql",
    "postgres"
  ],
  "author": {
    "name": "Qvalia AB, Sweden",
    "email": "info@qvalia.com",
    "url": "https://qvalia.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Qvalia/knex-aws-data-api/issues"
  },
  "homepage": "https://github.com/Qvalia/knex-aws-data-api#readme",
  "peerDependencies": {
    "knex": "^3.1.0",
    "@aws-sdk/client-rds-data": "^3.621.0",
    "pg": "8.12.0"
  },
  "devDependencies": {
    "rewire": "^7.0.0",
    "dotenv": "16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "knex-migrate": "1.7.4",
    "mysql": "2.18.1",
    "prettier": "^3.3.3"
  },
